<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AdminLoginServlet</servlet-name>
        <servlet-class>attire.packages.AdminLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductInfoUPServlet</servlet-name>
        <servlet-class>attire.packages.ProductInfoUPServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductImageServlet</servlet-name>
        <servlet-class>attire.packages.ProductImageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductDeleteServlet</servlet-name>
        <servlet-class>attire.packages.ProductDeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserLoginServlet</servlet-name>
        <servlet-class>attire.packages.UserLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserRegisterServlet</servlet-name>
        <servlet-class>attire.packages.UserRegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfilePhotoServlet</servlet-name>
        <servlet-class>attire.packages.ProfilePhotoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserPasswordUpdateServlet</servlet-name>
        <servlet-class>attire.packages.UserPasswordUpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserContactUpdateServlet</servlet-name>
        <servlet-class>attire.packages.UserContactUpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAddressUpdateServlet</servlet-name>
        <servlet-class>attire.packages.UserAddressUpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddtoCartServlet</servlet-name>
        <servlet-class>attire.packages.AddtoCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminChangePassServlet</servlet-name>
        <servlet-class>attire.packages.AdminChangePasswordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddReviewServlet</servlet-name>
        <servlet-class>attire.packages.AddReviewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCartItemServlet</servlet-name>
        <servlet-class>attire.packages.DeleteCartItemServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminLoginServlet</servlet-name>
        <url-pattern>/adminlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserLoginServlet</servlet-name>
        <url-pattern>/userLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRegisterServlet</servlet-name>
        <url-pattern>/userRegister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserPasswordUpdateServlet</servlet-name>
        <url-pattern>/updateUserPassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserContactUpdateServlet</servlet-name>
        <url-pattern>/updateusercontact</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAddressUpdateServlet</servlet-name>
        <url-pattern>/updateuseraddress</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddtoCartServlet</servlet-name>
        <url-pattern>/addtocart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminChangePassServlet</servlet-name>
        <url-pattern>/adminPassChange</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddReviewServlet</servlet-name>
        <url-pattern>/review</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCartItemServlet</servlet-name>
        <url-pattern>/deleteitem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductInfoUPServlet</servlet-name>
        <url-pattern>/uploadproductdetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductImageServlet</servlet-name>
        <url-pattern>/uploadProductImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductDeleteServlet</servlet-name>
        <url-pattern>/deleteproduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfilePhotoServlet</servlet-name>
        <url-pattern>/profilephoto</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
